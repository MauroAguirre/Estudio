package Algoritmo2;

public class Stack {
	private String[] stack;
	
	public Stack() {
		stack = new String[10];
	}
	public void Mostrar() {
		for(int i=0;i<10;i++) {
			if(stack[i]==null)
				break;
			System.out.println(stack[i]);
		}
	}
	public void Push(String s) {
		for(int i=stack.length-1;i>0;i--) {
			stack[i]=stack[i-1];
		}
		stack[0]=s;
	}
}
